{
    "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
    "version": 1,
    "newProjectRoot": "projects",
    "projects": {
        "ng-mode-switcher": {
            "projectType": "library",
            "root": "projects/ng-mode-switcher",
            "sourceRoot": "projects/ng-mode-switcher/src",
            "prefix": "lib",
            "architect": {
                "build": {
                    "builder": "@angular-devkit/build-angular:ng-packagr",
                    "options": {
                        "project": "projects/ng-mode-switcher/ng-package.json"
                    },
                    "configurations": {
                        "production": {
                            "tsConfig": "projects/ng-mode-switcher/tsconfig.lib.prod.json"
                        },
                        "development": {
                            "tsConfig": "projects/ng-mode-switcher/tsconfig.lib.json"
                        }
                    },
                    "defaultConfiguration": "production"
                },
                "test": {
                    "builder": "@angular-devkit/build-angular:karma",
                    "options": {
                        "main": "projects/ng-mode-switcher/src/test.ts",
                        "tsConfig": "projects/ng-mode-switcher/tsconfig.spec.json",
                        "karmaConfig": "projects/ng-mode-switcher/karma.conf.js"
                    }
                }
            }
        }
    }
}
